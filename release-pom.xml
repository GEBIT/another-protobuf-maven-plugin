  <developers>
    <developer>
      <id>sergei-ivanov</id>
      <name>Sergei Ivanov</name>
      <email>sergei@xolstice.org</email>
      <url>https://github.com/sergei-ivanov</url>
      <organization>Xolstice Ltd.</organization>
